{
  "mcpServers": {
    "deepwiki": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/sse",
      "alwaysAllow": [
        "ask_question"
      ],
      "disabled": false
    },
    "n8n-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "n8n-mcp@2.12.2"
      ],
      "env": {
        "N8N_API_URL": "${N8N_API_URL}",
        "N8N_API_KEY": "${N8N_API_KEY}",
        "LOG_LEVEL": "error"
      },
      "alwaysAllow": [
        "n8n_health_check",
        "validate_workflow",
        "n8n_create_workflow",
        "n8n_list_workflows",
        "n8n_update_partial_workflow",
        "tools_documentation",
        "n8n_get_workflow",
        "n8n_update_full_workflow",
        "get_templates_for_task",
        "search_nodes",
        "get_node_essentials",
        "validate_node_minimal",
        "n8n_diagnostic",
        "n8n_get_workflow_minimal",
        "validate_workflow_connections",
        "n8n_validate_workflow",
        "n8n_validate_workflow_connections",
        "validate_workflow_expressions",
        "validate_node_operation",
        "n8n_trigger_webhook_workflow",
        "n8n_list_executions"
      ],
      "disabled": false
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.3"
      ],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      },
      "alwaysAllow": [
        "tavily-search"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ],
      "alwaysAllow": [
        "get_current_time",
        "convert_time"
      ]
    },
    "cloudinary-asset-mgmt": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "@cloudinary/asset-management",
        "--",
        "mcp",
        "start"
      ],
      "env": {
        "CLOUDINARY_CLOUD_NAME": "${CLOUDINARY_CLOUD_NAME}",
        "CLOUDINARY_API_KEY": "${CLOUDINARY_API_KEY}",
        "CLOUDINARY_API_SECRET": "${CLOUDINARY_API_SECRET}"
      }
    },
    "cloudinary-env-config": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "@cloudinary/environment-config",
        "--",
        "mcp",
        "start"
      ],
      "env": {
        "CLOUDINARY_CLOUD_NAME": "${CLOUDINARY_CLOUD_NAME}",
        "CLOUDINARY_API_KEY": "${CLOUDINARY_API_KEY}",
        "CLOUDINARY_API_SECRET": "${CLOUDINARY_API_SECRET}"
      }
    },
    "cloudinary-smd": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "@cloudinary/structured-metadata",
        "--",
        "mcp",
        "start"
      ],
      "env": {
        "CLOUDINARY_CLOUD_NAME": "${CLOUDINARY_CLOUD_NAME}",
        "CLOUDINARY_API_KEY": "${CLOUDINARY_API_KEY}",
        "CLOUDINARY_API_SECRET": "${CLOUDINARY_API_SECRET}"
      }
    },
    "cloudinary-analysis": {
      "command": "npx",
      "args": [
        "-y",
        "--package",
        "@cloudinary/analysis",
        "--",
        "mcp",
        "start"
      ],
      "env": {
        "CLOUDINARY_CLOUD_NAME": "${CLOUDINARY_CLOUD_NAME}",
        "CLOUDINARY_API_KEY": "${CLOUDINARY_API_KEY}",
        "CLOUDINARY_API_SECRET": "${CLOUDINARY_API_SECRET}"
      }
    },
    "nocodetools": {
      "enabled": true,
      "owner": "admin",
      "type": "openapi",
      "openapi": {
        "version": "3.1.0",
        "url": "https://2281a5a294754c19f8c9e2df0be013fb-bobby-casper-4235.aiagentsaz.com/openapi.json"
      }
    },
    "neon-postgres": {
      "enabled": true,
      "owner": "admin",
      "type": "streamable-http",
      "url": "https://mcp.neon.tech/mcp",
      "headers": {
        "Authorization": "Bearer ${NEON_AUTH_TOKEN}"
      },
      "options": {
        "timeout": 10000
      }
    },
    "jina-ai": {
      "enabled": true,
      "owner": "admin",
      "type": "sse",
      "url": "https://mcp.jina.ai/sse",
      "headers": {
        "Authorization": "Bearer ${JINA_API_KEY}"
      },
      "keepAliveInterval": 60000
    },
    "openrouter": {
